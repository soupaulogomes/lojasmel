{
  "flex-layout.row#ajuda--tab-layout": {
    "title": "Ajuda - Menu",
    "children": ["tab-layout#ajuda"],
    "props": {
      "blockClass": "ajudaTabLayout"
    }
  },
  "tab-layout#ajuda": {
    "children": ["tab-list#ajuda", "tab-content#ajuda"],
    "props": {
      "blockClass": "ajudaTabLayout",
      "defaultActiveTabId": "ajuda--trocas-e-devolucoes"
    }
  },
  "tab-list#ajuda": {
    "children": [
      "tab-list.item#ajuda--como-comprar",
      "tab-list.item#ajuda--trocas-e-devolucoes",
      "tab-list.item#ajuda--garantias",
      "tab-list.item#ajuda--entregas",
      "tab-list.item#ajuda--retirada-em-loja",
      "tab-list.item#ajuda--perguntas-frequentes"
    ],
    "props": {
      "blockClass": "ajudaTabLayoutList"
    }
  },
  "tab-list.item#ajuda--como-comprar": {
    "title": "como-comprar",
    "props": {
      "tabId": "ajuda--como-comprar",
      "label": "Como comprar",
      "blockClass": "ajudaTabLayoutListItem"
    }
  },
  "tab-list.item#ajuda--trocas-e-devolucoes": {
    "title": "trocas-e-devolucoes",
    "props": {
      "tabId": "ajuda--trocas-e-devolucoes",
      "label": "Trocas e Devoluções",
      "blockClass": "ajudaTabLayoutListItem"
    }
  },
  "tab-list.item#ajuda--garantias": {
    "props": {
      "tabId": "ajuda--garantias",
      "label": "Garantias",
      "blockClass": "ajudaTabLayoutListItem"
    }
  },
  "tab-list.item#ajuda--entregas": {
    "props": {
      "tabId": "ajuda--entregas",
      "label": "Entregas",
      "defaultActiveTab": true,
      "blockClass": "ajudaTabLayoutListItem"
    }
  },
  "tab-list.item#ajuda--retirada-em-loja": {
    "props": {
      "tabId": "ajuda--retirada-em-loja",
      "label": "Retirada em Loja",
      "blockClass": "ajudaTabLayoutListItem"
    }
  },
  "tab-list.item#ajuda--perguntas-frequentes": {
    "props": {
      "tabId": "ajuda--perguntas-frequentes",
      "label": "Perguntas Frequentes",
      "blockClass": "ajudaTabLayoutListItem"
    }
  },
