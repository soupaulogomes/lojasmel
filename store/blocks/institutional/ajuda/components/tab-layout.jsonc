      "tab-content.item#ajuda--retirada-em-loja",
      "tab-content.item#ajuda--perguntas-frequentes"
    ],
    "props": {
      "blockClass": "ajudaTabLayoutContent"
    }
  },
  "tab-content.item#ajuda--como-comprar": {
    //meus pedidos
    "children": [
      "rich-text#ajuda--como-comprar-title",
